<h2>Login</h2>

<%= form_for :user, {url:sessions_path} do |f| %>
  <div><%= f.label :email %><br />
  <%= f.email_field :email, autofocus: true %></div><br />

  <div><%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off" %></div>

  <div><%= f.submit "Log in" %></div>
<% end %>